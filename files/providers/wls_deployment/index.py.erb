

def quote(text):
    if text:
        return "\"" + str(text).replace("\"", "\"\"") + "\""
    else: 
        return ""

f = open("/tmp/wlstScript.out", "w")
print >>f, "name;versionidentifier;deploymenttype;stagingmode;target;targettype;domain"

domainConfig()

