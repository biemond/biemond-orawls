try:
  security_crossdomain = str(get('CrossDomainSecurityEnabled'))
except:
  print "exception security_crossdomain", sys.exc_info()[0]
  security_crossdomain = '0'

cd('/JMX/'+domain_name)
jmx_platform_mbean_server_enabled = str(get('PlatformMBeanServerEnabled'))
