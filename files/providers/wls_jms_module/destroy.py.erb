connect('<%= weblogicUser %>','<%= weblogicPassword %>','<%= weblogicConnectUrl %>')

name    = '<%= name %>'

edit()
startEdit()

try:

    cd('/')
    cmo.destroyJMSSystemResource(getMBean('/SystemResources/'+name))

    save()
    activate()

    f = open("/tmp/wlstScript.out", "w")
    f.close()

except:
    print "Unexpected error:", sys.exc_info()[0]
    undo('true','y')
    stopEdit('y')
    raise

disconnect()
exit()
