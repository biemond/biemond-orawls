connect('<%= weblogicUser %>','<%= weblogicPassword %>','<%= weblogicConnectUrl %>')

name    = '<%= name %>'

edit()
startEdit()

try:

    cd('/')
    cmo.destroyJMSServer(getMBean('/Deployments/'+name))

    save()
    activate()

except:
    print "Unexpected error:", sys.exc_info()[0]
    undo('true','y')
    stopEdit('y')
    raise

disconnect()
exit()
