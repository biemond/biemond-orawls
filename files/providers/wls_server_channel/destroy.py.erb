connect('<%= weblogicUser %>','<%= weblogicPassword %>'  ,'<%= weblogicConnectUrl %>')

name                = '<%= channel_name %>'
server              = '<%= server %>'

edit()
startEdit()

try:

    cd('/Servers/'+server)
    cmo.destroyNetworkAccessPoint(getMBean('/Servers/'+server+'/NetworkAccessPoints/'+name))
    save()
    activate()

except:
    print "Unexpected error:", sys.exc_info()[0]
    undo('true','y')
    stopEdit('y')
    raise

disconnect()
exit()
