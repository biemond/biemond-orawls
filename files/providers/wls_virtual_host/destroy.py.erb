# check the domain else we need to skip this (done in wls_access.rb)
real_domain='<%= domain %>'



name                = '<%= virtual_host_name %>'

edit()
startEdit()

try:

    cd('/')
    cmo.destroyVirtualHost(getMBean('/VirtualHosts/'+name))
    save()
    activate()

except:
    print "Unexpected error:", sys.exc_info()[0]
    undo('true','y')
    stopEdit('y')
    raise

print "~~~~"
